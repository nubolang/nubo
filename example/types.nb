
struct User {
    id: int
    name: string
    weight: float
    gender: bool
    data: []byte
    initial: char
    other: dict[string, string]
    anyVal: any
    list: []any
    greet: fn(User) -> void
    custom: User?
    refUser: ref User
}

impl User {
    fn greet(self) {
        println(`Hello ${self.name}`)
    }
}

user = User{}

pub user.greet();
