import telnet from "@std/net/telnet"

const conn = telnet.Telnet("127.0.0.1", 2323)
defer conn.close()

while true {
    const out = conn.read(2000) // wait up to 2s
    if out != "" {
        println("Telnet got:", out)
    }
    conn.write("ping\n") // optional, send something
    sleep(1000) // sleep 1 second
}
